{
  "expo": {
    "name": "Qontact",
    "slug": "qontact",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "qontact",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#111418",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111418"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.qontact.app",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "backgroundColor": "#111418",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#111418"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "611184f4-07cd-476d-a009-eac5b3da865a"
      }
    }
  }
}
